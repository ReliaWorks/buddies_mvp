{
  "extends": "rallycoding",
  "rules": {
      "arrow-body-style": 0,
      "no-undef": 0,
      "no-underscore-dangle": 0,
      "comma-spacing": 0,
      "max-len": 0,
      "global-require": 0,
      "react/require-extension": 0,
      "object-shorthand": 0,
      "quotes": 0,
      "keyword-spacing": 0,
      "prefer-template": 0,
      "object-curly-spacing": 0,
      "no-else-return": 0,
      "no-restricted-syntax": 0,
      "react/sort-comp": 0,
      "curly": 0,
      "eqeqeq": 0,
      "no-unused-vars": 0,
  }
}
